        return 0
      fi;;
    esac

    case "$opt" in --gem-git-repo)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == gem ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == gem ]]; }; }; then
        return 0
      fi;;
    esac

    case "$opt" in --gem-git-branch)
