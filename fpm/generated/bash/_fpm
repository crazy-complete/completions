  local COMP_WORDBREAKS=''

  __complete_option() {
    local opt="$1" cur="$2"

    case "$opt" in --gem-bin-path)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == gem ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == gem ]]; }; }; then
        return 0
      fi;;
    esac

    case "$opt" in --gem-package-prefix)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == gem ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == gem ]]; }; }; then
        return 0
      fi;;
    esac

    case "$opt" in --gem-package-name-prefix)
