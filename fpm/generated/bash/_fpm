        _filedir
        return 0
      fi;;
    esac

    case "$opt" in --deb-interest)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == deb ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == deb ]]; }; }; then
        return 0
      fi;;
    esac

    case "$opt" in --deb-activate)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == deb ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == deb ]]; }; }; then
        return 0
      fi;;
    esac

    case "$opt" in --deb-interest-noawait)
