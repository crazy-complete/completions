        return 0
      fi;;
    esac

    case "$opt" in --cpan-package-name-prefix)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == cpan ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == cpan ]]; }; }; then
