        _filedir
        return 0
      fi;;
    esac

    case "$opt" in --rpm-pretrans)
      if { { (( ${#OPT_output_type[@]} )) && [[ "${OPT_output_type[-1]}" == rpm ]]; } || { (( ${#OPT_input_type[@]} )) && [[ "${OPT_input_type[-1]}" == rpm ]]; }; }; then
