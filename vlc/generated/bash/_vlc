    (( ! ${#OPT_longhelp[@]} )) && opts+=(--longhelp --no-longhelp)
    (( ! ${#OPT_help_verbose[@]} )) && opts+=(--help-verbose --no-help-verbose)
    (( ! ${#OPT_list[@]} )) && opts+=(-l --list --no-list)
    (( ! ${#OPT_list_verbose[@]} )) && opts+=(--list-verbose --no-list-verbose)
