      --global-key-set-bookmark9) VAR=OPT_global_key_set_bookmark9; ARGS=1; return;;
      --key-set-bookmark9) VAR=OPT_key_set_bookmark9; ARGS=1; return;;
      --global-key-set-bookmark10) VAR=OPT_global_key_set_bookmark10; ARGS=1; return;;
      --key-set-bookmark10) VAR=OPT_key_set_bookmark10; ARGS=1; return;;
      --global-key-play-bookmark1) VAR=OPT_global_key_play_bookmark1; ARGS=1; return;;
      --key-play-bookmark1) VAR=OPT_key_play_bookmark1; ARGS=1; return;;
      --global-key-play-bookmark2) VAR=OPT_global_key_play_bookmark2; ARGS=1; return;;
      --key-play-bookmark2) VAR=OPT_key_play_bookmark2; ARGS=1; return;;
      --global-key-play-bookmark3) VAR=OPT_global_key_play_bookmark3; ARGS=1; return;;
      --key-play-bookmark3) VAR=OPT_key_play_bookmark3; ARGS=1; return;;
      --global-key-play-bookmark4) VAR=OPT_global_key_play_bookmark4; ARGS=1; return;;
      --key-play-bookmark4) VAR=OPT_key_play_bookmark4; ARGS=1; return;;
      --global-key-play-bookmark5) VAR=OPT_global_key_play_bookmark5; ARGS=1; return;;
      --key-play-bookmark5) VAR=OPT_key_play_bookmark5; ARGS=1; return;;
      --global-key-play-bookmark6) VAR=OPT_global_key_play_bookmark6; ARGS=1; return;;
      --key-play-bookmark6) VAR=OPT_key_play_bookmark6; ARGS=1; return;;
      --global-key-play-bookmark7) VAR=OPT_global_key_play_bookmark7; ARGS=1; return;;
      --key-play-bookmark7) VAR=OPT_key_play_bookmark7; ARGS=1; return;;
      --global-key-play-bookmark8) VAR=OPT_global_key_play_bookmark8; ARGS=1; return;;
      --key-play-bookmark8) VAR=OPT_key_play_bookmark8; ARGS=1; return;;
      --global-key-play-bookmark9) VAR=OPT_global_key_play_bookmark9; ARGS=1; return;;
      --key-play-bookmark9) VAR=OPT_key_play_bookmark9; ARGS=1; return;;
      --global-key-play-bookmark10) VAR=OPT_global_key_play_bookmark10; ARGS=1; return;;
      --key-play-bookmark10) VAR=OPT_key_play_bookmark10; ARGS=1; return;;
      --global-key-clear-playlist) VAR=OPT_global_key_clear_playlist; ARGS=1; return;;
      --key-clear-playlist) VAR=OPT_key_clear_playlist; ARGS=1; return;;
      --global-key-subtitle-text-scale-normal) VAR=OPT_global_key_subtitle_text_scale_normal; ARGS=1; return;;
      --key-subtitle-text-scale-normal) VAR=OPT_key_subtitle_text_scale_normal; ARGS=1; return;;
      --global-key-subtitle-text-scale-up) VAR=OPT_global_key_subtitle_text_scale_up; ARGS=1; return;;
      --key-subtitle-text-scale-up) VAR=OPT_key_subtitle_text_scale_up; ARGS=1; return;;
      --global-key-subtitle-text-scale-down) VAR=OPT_global_key_subtitle_text_scale_down; ARGS=1; return;;
      --key-subtitle-text-scale-down) VAR=OPT_key_subtitle_text_scale_down; ARGS=1; return;;
      --bookmark1) VAR=OPT_bookmark1; ARGS=1; return;;
      --bookmark2) VAR=OPT_bookmark2; ARGS=1; return;;
      --bookmark3) VAR=OPT_bookmark3; ARGS=1; return;;
      --bookmark4) VAR=OPT_bookmark4; ARGS=1; return;;
      --bookmark5) VAR=OPT_bookmark5; ARGS=1; return;;
      --bookmark6) VAR=OPT_bookmark6; ARGS=1; return;;
      --bookmark7) VAR=OPT_bookmark7; ARGS=1; return;;
      --bookmark8) VAR=OPT_bookmark8; ARGS=1; return;;
      --bookmark9) VAR=OPT_bookmark9; ARGS=1; return;;
      --bookmark10) VAR=OPT_bookmark10; ARGS=1; return;;
      -h|--help|--no-help) VAR=OPT_help; ARGS=0; return;;
      -H|--full-help|--no-full-help) VAR=OPT_full_help; ARGS=0; return;;
      --longhelp|--no-longhelp) VAR=OPT_longhelp; ARGS=0; return;;
      --help-verbose|--no-help-verbose) VAR=OPT_help_verbose; ARGS=0; return;;
      -l|--list|--no-list) VAR=OPT_list; ARGS=0; return;;
      --list-verbose|--no-list-verbose) VAR=OPT_list_verbose; ARGS=0; return;;
      -p|--module) VAR=OPT_module; ARGS=1; return;;
      --ignore-config|--no-ignore-config) VAR=OPT_ignore_config; ARGS=0; return;;
      --reset-config|--no-reset-config) VAR=OPT_reset_config; ARGS=0; return;;
      --reset-plugins-cache|--no-reset-plugins-cache) VAR=OPT_reset_plugins_cache; ARGS=0; return;;
      --version|--no-version) VAR=OPT_version; ARGS=0; return;;
      --config) VAR=OPT_config; ARGS=1; return;;
    esac
    return 1
  }

  __append_to_array() {
    local -n arr=$1
    arr+=("$2")
  }

  for ((argi=1; argi < ${#words[@]} - 1; ++argi)); do
    arg="${words[argi]}"

    case "$arg" in
      --)
        END_OF_OPTIONS=1
        POSITIONALS+=("${words[@]:$((++argi))}")
        return;;
      --*=*)
        if __find_option "$cmd" "${arg%%=*}"
        then __append_to_array "$VAR" "${arg#*=}"
        fi;;
      --*)
        if __find_option "$cmd" "$arg"; then
          if [[ "$ARGS" == 1 ]]
          then __append_to_array "$VAR" "${words[++argi]}"
          else __append_to_array "$VAR" "_OPT_ISSET_"
          fi
        fi;;
      -?*) # ignore '-'

        for ((i=1; i < ${#arg}; ++i)); do
          char="${arg:$i:1}"
          trailing_chars="${arg:$((i + 1))}"

          if __find_option "$cmd" "-$char"; then
            if [[ "$ARGS" == 1 ]]; then
              if [[ -n "$trailing_chars" ]]
              then __append_to_array "$VAR" "$trailing_chars"
              else __append_to_array "$VAR" "${words[++argi]}"
              fi
              break;
            else
              __append_to_array "$VAR" "_OPT_ISSET_"
            fi
          fi
        done
        ;;
      *)
        POSITIONALS+=("$arg")
        ;;
    esac
  done

  for ((; argi < ${#words[@]}; ++argi)); do
    case "${words[argi]}" in
      -?*);;
      *) POSITIONALS+=("${words[argi]}");;
    esac
  done
}

