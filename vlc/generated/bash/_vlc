    (( ! ${#OPT_reset_config[@]} )) && opts+=(--reset-config --no-reset-config)
    (( ! ${#OPT_reset_plugins_cache[@]} )) && opts+=(--reset-plugins-cache --no-reset-plugins-cache)
    (( ! ${#OPT_version[@]} )) && opts+=(--version --no-version)
    (( ! ${#OPT_config[@]} )) && opts+=(--config=)
    COMPREPLY+=($(compgen -W "${opts[*]}" -- "$cur"))
    [[ ${COMPREPLY-} == *= ]] && compopt -o nospace
    return 1
  fi

  (( ${#POSITIONALS[@]} >= 1 )) && {
    _filedir
    return 0;
  }

  return 1
}

complete -F _vlc vlc

