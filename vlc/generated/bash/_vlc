-bookmark6|--key-set-bookmark6|--global-key-set-bookmark7|--key-set-bookmark7|--global-key-set-bookmark8|--key-set-bookmark8|--global-key-set-bookmark9|--key-set-bookmark9|--global-key-set-bookmark10|--key-set-bookmark10|--global-key-play-bookmark1|--key-play-bookmark1|--global-key-play-bookmark2|--key-play-bookmark2|--global-key-play-bookmark3|--key-play-bookmark3|--global-key-play-bookmark4|--key-play-bookmark4|--global-key-play-bookmark5|--key-play-bookmark5|--global-key-play-bookmark6|--key-play-bookmark6|--global-key-play-bookmark7|--key-play-bookmark7|--global-key-play-bookmark8|--key-play-bookmark8|--global-key-play-bookmark9|--key-play-bookmark9|--global-key-play-bookmark10|--key-play-bookmark10|--global-key-clear-playlist|--key-clear-playlist|--global-key-subtitle-text-scale-normal|--key-subtitle-text-scale-normal|--global-key-subtitle-text-scale-up|--key-subtitle-text-scale-up|--global-key-subtitle-text-scale-down|--key-subtitle-text-scale-down)
        _vlc__compgen_w_replacement -- "$cur" A alt-A shift-A ctrl-A meta-A B alt-B shift-B ctrl-B meta-B C alt-C shift-C ctrl-C meta-C D alt-D shift-D ctrl-D meta-D E alt-E shift-E ctrl-E meta-E F alt-F shift-F ctrl-F meta-F G alt-G shift-G ctrl-G meta-G H alt-H shift-H ctrl-H meta-H I alt-I shift-I ctrl-I meta-I J alt-J shift-J ctrl-J meta-J K alt-K shift-K ctrl-K meta-K L alt-L shift-L ctrl-L meta-L M alt-M shift-M ctrl-M meta-M N alt-N shift-N ctrl-N meta-N O alt-O shift-O ctrl-O meta-O P alt-P shift-P ctrl-P meta-P Q alt-Q shift-Q ctrl-Q meta-Q R alt-R shift-R ctrl-R meta-R S alt-S shift-S ctrl-S meta-S T alt-T shift-T ctrl-T meta-T U alt-U shift-U ctrl-U meta-U V alt-V shift-V ctrl-V meta-V W alt-W shift-W ctrl-W meta-W X alt-X shift-X ctrl-X meta-X Y alt-Y shift-Y ctrl-Y meta-Y Z alt-Z shift-Z ctrl-Z meta-Z F1 alt-F1 shift-F1 ctrl-F1 meta-F1 F2 alt-F2 shift-F2 ctrl-F2 meta-F2 F3 alt-F3 shift-F3 ctrl-F3 meta-F3 F4 alt-F4 shift-F4 ctrl-F4 meta-F4 F5 alt-F5 shift-F5 ctrl-F5 meta-F5 F6 alt-F6 shift-F6 ctrl-F6 meta-F6 F7 alt-F7 shift-F7 ctrl-F7 meta-F7 F8 alt-F8 shift-F8 ctrl-F8 meta-F8 F9 alt-F9 shift-F9 ctrl-F9 meta-F9 F10 alt-F10 shift-F10 ctrl-F10 meta-F10 F11 alt-F11 shift-F11 ctrl-F11 meta-F11 F12 alt-F12 shift-F12 ctrl-F12 meta-F12 Up alt-Up shift-Up ctrl-Up meta-Up Down alt-Down shift-Down ctrl-Down meta-Down Left alt-Left shift-Left ctrl-Left meta-Left Right alt-Right shift-Right ctrl-Right meta-Right End alt-End shift-End ctrl-End meta-End Home alt-Home shift-Home ctrl-Home meta-Home Space alt-Space shift-Space ctrl-Space meta-Space Enter alt-Enter shift-Enter ctrl-Enter meta-Enter Tab alt-Tab shift-Tab ctrl-Tab meta-Tab Esc alt-Esc shift-Esc ctrl-Esc meta-Esc 'Page Up' 'alt-Page Up' 'shift-Page Up' 'ctrl-Page Up' 'meta-Page Up' 'Page Down' 'alt-Page Down' 'shift-Page Down' 'ctrl-Page Down' 'meta-Page Down' Backspace alt-Backspace shift-Backspace ctrl-Backspace meta-Backspace Insert alt-Insert shift-Insert ctrl-Insert meta-Insert Delete alt-Delete shift-Delete ctrl-Delete meta-Delete
        return 0;;
    esac

    return 1
  }

  case "$prev" in
    --*) __complete_option "$prev" "$cur" && return 0;;
    -*([qfZLRdhHl])[AVSvIp])
         __complete_option "-${prev: -1}" "$cur" && return 0;;
  esac

  case "$cur" in
    --*=*)
      __complete_option "${cur%%=*}" "${cur#*=}" && return 0;;
    --*);;
    -*=*);;
    -*([qfZLRdhHl])[AVSvIp]*)
      local i
      for ((i=2; i <= ${#cur}; ++i)); do
        local pre="${cur:0:$i}" value="${cur:$i}"
        __complete_option "-${pre: -1}" "$value" && {
          _vlc__prefix_compreply "$pre"
          return 0
        }
      done;;
  esac

